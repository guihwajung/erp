				this.dsTAX_MONTH.setColumn(3, "CD_CODE", "4");
				this.dsTAX_MONTH.setColumn(3, "DS_CODE", "3월");
				this.dsSearch.setColumn(0, "TAX_MONTH", "1");
			
				dtFrom = ymMagam + "-01-01";
				dtTo = ymMagam + "-03-31";
				break;
			case "2" :
				this.dsTAX_MONTH.setColumn(0, "CD_CODE", "5");
