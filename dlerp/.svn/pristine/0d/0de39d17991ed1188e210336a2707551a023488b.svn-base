this.fnCallback = function(svcID, errorCode, errorMsg)
{
	if (svcID == "select") {
		this.gfnGridAfterSelect(this.dxGrid);
	}
	else if (svcID == "detail") {
		
		this.gfnGridAfterSelect(this.dxGridSub);	
	}
	else if(svcID == "cancel") {
