	// 집계 조회
	this.dsRpt = new Dataset();
	this.dsRpt.addColumn("DT_SINCHUNG_FR", "string");
	this.dsRpt.addColumn("DT_SINCHUNG_TO", "string");
	this.dsRpt.addColumn("ID_LOGIN", "string");	
}

