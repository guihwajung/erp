	this.dsSave.setColumn(nrow, "DT_BIRTH", this.dsList.getColumn(0, "DT_BIRTH"));
	this.dsSave.setColumn(nrow, "NO_BUSU_REQUEST", this.dsList.getColumn(0, "NO_BUSU_REQUEST"));
