};

this.btnConf_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.dsList.rowcount == 0 && this.dsList2.rowcount == 0)
		return;

	this.fnSave();
};
]]></Script>
