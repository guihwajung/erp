 *	콜백 처리
 */
this.fnCallback = function(svcID, errorCode, errorMsg)
{
	if(errorCode != 0) {
		this.gfnAlert(errorMsg);
		return;
	}
	
	if (svcID == "select") 
	{
		this.fnSetGrid();
		this.dsList.copyData(this.dsResult);
	}
}



