        this.fnAdd = function() {
        	var nrow = this.gfnGridAdd(this.dxGridSub, "bottom"); // top (default), bottom, current

        	this.dsListSub.set_enableevent(false);
        	this.dsListSub.setColumn(nrow, "YN_EDIT", "Y");
        	this.dsListSub.set_enableevent(true);
        }

        /*
