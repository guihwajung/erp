	else {
		var bTY_CONTRACTOR = this.dsList.getColumn(0, "TY_CONTRACTOR") == "P";
		
		this.txtDS_VENDOR.set_readonly(bTY_CONTRACTOR);
		this.txtDS_UPJONG.set_readonly(bTY_CONTRACTOR);
