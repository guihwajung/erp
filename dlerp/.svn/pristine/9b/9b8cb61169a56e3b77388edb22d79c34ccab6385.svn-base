	if (this.tabData.tabindex != 0) return;
	if (this.gfnIsNull(this.dsSearch.getColumn(0, "CD_ACNTUNIT"))) return;
	
	if (this.dsList1.rowcount == 0) {
		this.gfnAlert("부과된 수납내역이 없으므로, 수납취소를 할 수 없습니다!");
		return;
