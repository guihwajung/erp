 this.dsList_onvaluechanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "CD_VACATION"){
		// 휴가구분이 변경될경우 휴가기간/구분/일수 빈값 리셋		
		if(e.oldvalue != e.newvalue) {
			obj.setColumn(e.row,"DT_VACATO","");
			obj.setColumn(e.row,"DT_VACAFROM","");

			this.fnSetVacationForm(e.newvalue);
			this.fnSetVacationData(e.newvalue);			
			//obj.setColumn(e.row,"AM_VAT" , nexacro.toNumber(obj.getColumn(e.row,"AM_SUPPLY"),0) * (nexacro.toNumber(obj.getColumn(e.row,"PER_VATPROOF"),0)/100));

		}
	}
	
	if(e.columnid == "DT_VACAFROM"){
		// FROM ~ TO가 완료 되면 날짜 계산
		var sCdVacation = obj.getColumn(e.row,"CD_VACATION");
		var sDtVacaFrom = e.newvalue;
		var sDtVacaTo = obj.getColumn(e.row,"DT_VACATO");

		// 휴가일수 계산
		//trace("FROM 휴가일수 계산->" + sDtVacaFrom + " / " + sDtVacaTo);
		if(!this.gfnIsNull(sDtVacaFrom) && !this.gfnIsNull(sDtVacaTo) ){
			if(sCdVacation == "A2"){
				obj.setColumn(e.row,"QN_VACATION", "0.5");
			}else{
				this.fnSelectQnDay();
				// EXEC DAZPR_DAYSUM 		
				// SP에서 리턴 받는 값을 일수에 넣어준다. 
				// 	DAZPR_DAYSUM	
				// 	@TY_GUBUN	휴가구분
				// 	@DT_START	시작일
				// 	@DT_END	종료일
				// 	@QN_DAY	 OUTPUT   일수
			}
		}else{
			obj.setColumn(e.row,"QN_VACATION", "");
		}
	}
	
	if(e.columnid == "DT_VACATO"){
		// FROM ~ TO가 완료 되면 날짜 계산
		var sCdVacation = obj.getColumn(e.row,"CD_VACATION");
		var sDtVacaFrom = obj.getColumn(e.row,"DT_VACAFROM");
		var sDtVacaTo = e.newvalue;

		if(sCdVacation == "A2"){	// 반차
			obj.setColumn(e.row,"DT_VACAFROM", sDtVacaTo);
			return;
		}

		// 휴가일수 계산
		if(!this.gfnIsNull(sDtVacaFrom) && !this.gfnIsNull(sDtVacaTo) ){
			if(sCdVacation == "A2"){
				obj.setColumn(e.row,"QN_VACATION", "0.5");
			}else{
				this.fnSelectQnDay();
				// EXEC DAZPR_DAYSUM 		
				// SP에서 리턴 받는 값을 일수에 넣어준다. 
				// 	DAZPR_DAYSUM	
				// 	@TY_GUBUN	휴가구분
				// 	@DT_START	시작일
				// 	@DT_END	종료일
				// 	@QN_DAY	 OUTPUT   일수
			}
		}else{
			obj.setColumn(e.row,"QN_VACATION", "");
		}
	}	
}

