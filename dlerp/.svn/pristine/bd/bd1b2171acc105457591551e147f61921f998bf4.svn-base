					this.gfnSetAllEnable(this.divData1.form.divDataInfo.form, false);
					this.ctclDT_VACAFROM.set_enable(false);
					this.ctclDT_VACATO.set_enable(false);
				}else{
					// 00 작성중 / 04 반려 / 05 결재취소
					this.gfnSetAllEnable(this.divData1.form.divDataInfo.form, true);
				}
				
			}
			
		} else {
			this.gfnAlert(errorMsg);
		}
	}
	else if(svcID == "selectQnDay") {
		if (errorCode == 0) {
			this.dsList.setColumn(this.dsList.rowposition,"QN_VACATION", this.dsListQnDay.getColumn(0, "QN_DAY"));
		} else {
			this.gfnAlert(errorMsg);
		}
	}
}

