// 				this.txtDS_ETITLE.set_readonly(false);
// 			else
// 				this.txtDS_ETITLE.set_readonly(true);

			
			this.fnSelect();
		}
	}
	else if(svcID == "savemaster") {
		if (errorCode == 0) {
			var json = this.gfnArgsToJson(strArg);
			var status = json.status;

			//this.fnExec("01");
			this.fnSave(status);
		} else {
			this.gfnAlert(errorMsg);
		}
	}
