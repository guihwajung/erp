        {
        	if(obj.getColumn(e.newrow, "LV_ACNT") >= 3){
        		this.gfnSetAllEnable(this.divData.form.divDataRightTop.form, true);
        		this.gfnSetAllEnable(this.divData.form.divDataRightBottom.form, true);
        	}else{
        		this.gfnSetAllEnable(this.divData.form.divDataRightTop.form, false);
        		this.gfnSetAllEnable(this.divData.form.divDataRightBottom.form, false);
        	}

        	this.gfnSetEnable(this.divData.form.divDataRightBottom.form.chkYN_DATE, false);
        	this.gfnSetEnable(this.divData.form.divDataRightBottom.form.chkYN_VENDOR, false);

        	//미수미불구분 처리
        	this.fnSetTyMigeulEnable(obj.getColumn(e.newrow, "YN_MISU_MIBUL_ACNT"));
        };

        this.dsList_oncolumnchanged = function(obj,e)
        {
        	if(e.columnid == "YN_MISU_MIBUL_ACNT"){	//미수미불구분
        		if(e.newvalue != e.oldvalue){
        			this.fnSetTyMigeulEnable(e.newvalue);
        			obj.setColumn(e.row, "TY_MIGEUL", "");
        		}
        	}

        	if(e.columnid == "YN_TRIAL"){	//시산과목
        		if(e.newvalue != e.oldvalue){
        		trace(" YN_TRIAL =>" + obj.getColumn(e.row, "YN_TRIAL"));
        			if (obj.getColumn(e.row, "YN_TRIAL") == "Y" && this.gfnIsNull(obj.getColumn(e.row, "CD_TRIAL")) )
        			{
        				obj.setColumn(e.row, "CD_TRIAL", obj.getColumn(e.row, "CD_ACCOUNT"));
        			}
        			if (obj.getColumn(e.row, "YN_TRIAL") == "N" && !this.gfnIsNull(obj.getColumn(e.row, "CD_TRIAL")) &&
        				obj.getColumn(e.row, "CD_TRIAL") == obj.getColumn(e.row, "CD_ACCOUNT"))
        			{
        				obj.setColumn(e.row, "CD_TRIAL", "");
        			}
        		}
        	}
        };

