        			this.divData.form.divDataRightBottom.form.ccboCK_VENDOR_MO.setFocus();
        			return false;
        		}
        	}

        	if(this.dsList.getColumn(pRow, "YN_ETC1") == "Y"){
        		if(this.gfnIsNull(this.gfnTrim(this.dsList.getColumn(pRow, "CK_ETC1_MO")))){
        			this.gfnAlert("보조부 관리유형에 기타1별이 선택되었을때는   ☞ 명칭정보의 기타1 필수,선택을 입력하셔야 합니다.");
        			this.divData.form.divDataRightBottom.form.ccboCK_ETC1_MO.setFocus();
        			return false;
        		}
