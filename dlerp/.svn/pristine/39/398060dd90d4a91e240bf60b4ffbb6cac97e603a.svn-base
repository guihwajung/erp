        this.fnDataValidate = function(){
        	var pRow = this.dsList.rowposition;

        	// 시산과목
        	var YN_TRIAL = this.gfnTrim(this.dsList.getColumn(pRow, "YN_TRIAL"));
        	if(this.gfnIsNull(this.gfnTrim(this.dsList.getColumn(pRow, "CD_ACCOUNT")))){
        		this.gfnAlert("계정코드는 필수입력입니다.");
        		this.divData.form.divDataRightTop.form.txtCD_ACCOUNT.setFocus();
        		return false;
        	}

        	if(this.gfnIsNull(this.gfnTrim(this.dsList.getColumn(pRow, "DS_ACCOUNT")))){
        		this.gfnAlert("계정명칭은 필수입력입니다.");
        		this.divData.form.divDataRightTop.form.txtDS_ACCOUNT.setFocus();
        		return false;
        	}

        	if(YN_TRIAL == "G"){	// 시산과목(N:과목Y:시산G:과목)
        	}else{
        		if(this.gfnIsNull(this.gfnTrim(this.dsList.getColumn(pRow, "CD_TRIAL")))){
        			this.gfnAlert("시산코드는 필수입력입니다.");
        			this.ccfCD_TRIAL.form.CDTextBox.setFocus();
        			return false;
        		}
