        			this.divData.form.divDataRightBottom.form.chkYN_VENDOR.setFocus();
        			return false;
        		}
        		if(this.gfnIsNull(this.gfnTrim(this.dsList.getColumn(pRow, "TY_MIGEUL")))){
        			this.gfnAlert("미수/미불관리를 하는 계정은 미수미불구분을 선택하셔야 합니다.");
        			this.divData.form.divDataRightBottom.form.ccboTY_MIGEUL.setFocus();
        			return false;
        		}
        	}
        	if(this.dsList.getColumn(pRow, "YN_VENDOR") == "Y"){
