            } else if (ty_gubun == 6) {
                fileColName = "LOGO3_IMG";
            }

            this.dsDeleteFile.setColumn(0, "filepath", this.filepath + key);
            this.dsDeleteFile.setColumn(0, "filename", this.dsDetail.getColumn(0, fileColName));


            // 서버 파일 삭제
            if (this.dsDeleteFile.rowcount > 0) {
