	json.DS_EDU_ORGAN = this.dsData.getColumn(0, "DS_EDU_ORGAN");
	json.AM_EDUCATION = this.dsData.getColumn(0, "AM_EDUCATION");

	this.getParentContext().close(JSON.stringify(json));
}

