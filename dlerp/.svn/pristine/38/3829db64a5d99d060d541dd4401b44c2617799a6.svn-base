	json.FR_EDUCATION = this.dsData.getColumn(0, "FR_EDUCATION");
	json.TO_EDUCATION = this.dsData.getColumn(0, "TO_EDUCATION");
	json.CD_EDU_INOUT = this.dsData.getColumn(0, "CD_EDU_INOUT");
