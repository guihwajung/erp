			}
			if (obj.getColumn(e.row, "YN_TRIAL") == "N" && !this.gfnIsNull(obj.getColumn(e.row, "CD_TRIAL")) && 
				obj.getColumn(e.row, "CD_TRIAL") == obj.getColumn(e.row, "CD_ACCOUNT")) 
			{
				obj.setColumn(e.row, "CD_TRIAL", "");
			}
		}
	}
};

