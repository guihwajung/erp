 ************************************************************************/
this.fnCallback = function(svcID, errorCode, errorMsg)
{
	if (errorCode != 0) {
		this.gfnAlert(errorMsg);
		return;
	} 
	
	if(svcID == "select") {
		if(this.dsList1.rowcount == 0)
		{
			this.dsList1.addRow();
		}
	}
	else if(svcID == "save"){ 	
		if (errorCode == 0) { 	
			this.getParentContext().close(true);
			
		} else {
			this.gfnAlert(errorMsg);
		}
	}
}


