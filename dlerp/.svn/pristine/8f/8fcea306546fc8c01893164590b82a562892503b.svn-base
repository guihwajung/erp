	if (!val) return;

	var json = JSON.parse(val);
	if (json.DONG.length > 0) {
		this.dsList.setColumn(0, "NO_DONG", json.DONG);
		this.dsList.setColumn(0, "NO_FLOOR", json.FLOOR);
		this.dsList.setColumn(0, "NO_HO", json.HO);
