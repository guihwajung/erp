              <Static id="staDT_YM" taborder="0" text="년월" left="0.0" top="10.0" width="92.0" height="24.0" cssclass="sta_WF_SchLabelE"/>
              <Div id="ctclDT_YM" taborder="1" left="staDT_YM:0.0" top="10.0" width="80" height="24.0" url="cmm::cmmCalMM.xfdl"/>
              <Static id="staTY_SAVE" taborder="2" text="저장형태" left="ctclDT_YM:0.0" top="10.0" width="92.0" height="24.0" textDecoration="none" cssclass="sta_WF_SchLabel"/>
