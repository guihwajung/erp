			{
				case "I" :
					this.gfnAlert("입력이 완료되었습니다.", "", "", "fnInsert_callback");
					break;
				case "U" : 
					this.FormBtns.Select.click();
					break;
				case "D" : 
					this.gfnAlert("삭제가 완료되었습니다.", "" , "" , "fnClose_callback");
					break;
			}
		}
	}
	else
	{
		this.gfnAlert(errorMsg);
	}
}

