				this.dsUpdate.setColumn(nrow, "DT_CANCEL", this.dsList.getColumn(i, "DT_CANCEL"));
				this.dsUpdate.setColumn(nrow, "TY_EXPIRY", this.dsList.getColumn(i, "TY_EXPIRY"));
