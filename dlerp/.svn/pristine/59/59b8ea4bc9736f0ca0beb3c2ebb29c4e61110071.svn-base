	var aTabDir = ["","ADDRESS", "FAMILY", "SCHOOL", "LICENSE", "CAREER","","LANGUAGE"];	// 기본정보, 주소,가족,학력,자격,경력, 기타, 외국어
	var sCdDir = "";

	if(colnm == "NM_FILE") {
		// 최초 작성시엔 NO_SEQ가 없기 때문에 저장 후에 파일 첨부를 진행하여야 함.
		// CD_DIR에 넣어줄 값이 없기에 저장 후 진행!
