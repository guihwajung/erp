					this.dsDeleteSub.setColumn(nrow, "CD_VENDOR", this.dsList3.getColumn(i, "CD_VENDOR"));
					break;
			}
		}
		
		if (cntDangSa > 1){
			this.gfnAlert("당사가 이미 존재합니다!");
			return;
		}
		if (cntDAEPYO > 1){
			this.gfnAlert("대표거래처는 하나만 존재해야합니다!");
			return;
		}
		if (jiBunYul > 100){
			this.gfnAlert("지분율 입력이 잘못되었습니다! 지분율이 100%를 초과했습니다!");
			return;
		}
