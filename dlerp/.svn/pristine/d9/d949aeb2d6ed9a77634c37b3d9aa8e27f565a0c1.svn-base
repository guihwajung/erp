			}
			
			if(this.dsList3.getColumn(i, "YN_DAEPYO") == "대표" || this.dsList3.getColumn(i, "YN_DAEPYO") == "Y"){
				cntDAEPYO += 1;
			}
			
			jiBunYul += this.dsList3.getColumn(i, "RT_JIBUN");
						
