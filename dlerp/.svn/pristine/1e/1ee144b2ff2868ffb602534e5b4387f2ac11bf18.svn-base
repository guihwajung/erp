						false);
	
	for (var i = 0; i < this.dsListRptProof.rowcount; i++) {
		var CD_PMT_TYPE = this.dsListRptProof.getColumn(i, "CD_PMT_TYPE");
		var CNT_FILE = this.dsListRptProof.getColumn(i, "CNT_FILE");
		
