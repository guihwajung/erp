        	this.ccfCD_CORP = this.divSearch.form.ccfCD_CORP;
        	this.ctclDT_FROM = this.divSearch.form.ctclDT_FROM;
        	this.ctclDT_TO = this.divSearch.form.ctclDT_TO;

        	this.dxGrid = this.divData.form.objGrid;

        };

        /************************************************************************
