        /*
         *	콜백 처리
         */
        this.fnCallback = function(svcID, errorCode, errorMsg, strArg)
        {
        	//조회완료 후
        	if( svcID == "select" ){
        		//사직서 작성 버튼 클릭 시
        		if(this.getOwnerFrame().NO_SEQ == 0) {
        			var msg = this.dsList.getColumn(0, "MSG");
        			if( this.gfnIsNull(msg)){
        				//자료가 미 존재시 기본정보 셋팅
        				this.divData.form.staTile02_00_00_00_00_00.set_visible(false);
        				this.divData.form.objGrid.set_visible(false);
        				//결재 비활성화
        				this.divData.form.btnAprv.set_enable(false);
        				this.divData.form.btnAprvConnect.set_enable(false);
        				this.divData.form.btnInit.set_enable(false);

        				//퇴직담당자 지정 팝업 호출
        				this.fnPopup();
        			}else{
        				//a메세지 전송 후 종료
        				var param = {};
        				param.msg = msg;
        				this.getParentContext().close(JSON.stringify(param));
        			}
        			var tyJiwon = this.dsList.getColumn(0, "TY_JIWON"); //지원담당자 선택여부
        			//널이거나 0이면 전자결재 지원 담당자 비활성화
        			if(this.gfnIsNull(tyJiwon) ||  tyJiwon == "0" ){
        				this.divData.form.cfCD_JIWON_SABUN.set_enable(false);
        			}
        		}else{
        		   var empBirthDay = this.dsList.getColumn(0, "EMPLOYMENT_BIRTHDAY");
        		   var birthDay = this.dsList.getColumn(0, "DT_BIRTHDAY");

        		   this.divData.form.tclEMPLOYMENT_BIRTHDAY.set_value(empBirthDay);
        		   this.divData.form.tclDT_BIRTHDAY.set_value(birthDay);

        			var tyJiwon = this.dsList.getColumn(0, "TY_JIWON"); //지원담당자 선택여부
        			//1인 경우만 셋팅
        			if( tyJiwon == "1" ){
        				this.divData.form.txtDS_APPRVAL_LINE.set_value(this.dsList.getColumn(0, "DS_APPRVAL_LINE"));
        			}
        			//널이거나 0이면 전자결재 지원 담당자 비활성화
        			if(this.gfnIsNull(tyJiwon) ||  tyJiwon == "0" ){
        				this.divData.form.cfCD_JIWON_SABUN.set_enable(false);
        			}
        			/**************************************  버튼 컨트롤 (START)  **********************************/
        			//버튼 상태
        			var cdButtonStats = this.dsList.getColumn(0, "CD_BUTTON_STATS");
        			//결재문서 상신, 조회, 초기화 설정
        			this.divData.form.btnAprv.set_enable(false);
        			this.divData.form.btnAprvConnect.set_enable(false);
        			this.divData.form.btnInit.set_enable(false);

        			this.divData.form.btnSave.set_enable(false);
        			this.divData.form.btnApply.set_enable(false);
        			//신청 및 저장 버튼 활성화 나머지 비활성화
        			if( cdButtonStats == "1" ){
        				this.divData.form.btnSave.set_enable(true);
        				this.divData.form.btnApply.set_enable(true);
        			}
        			//저장 버튼 활성화 나머지 비활성화
        			else{
        				this.divData.form.btnSave.set_enable(true);
        			}

        			var btnAprv = this.dsList.getColumn(0, "BTN_APRV");
        			var btnDocQ = this.dsList.getColumn(0, "BTN_DOC_Q");
        			var btnInit = this.dsList.getColumn(0, "BTN_INIT");

        			//상신
        			if( btnAprv == "Y" ){
        				this.divData.form.btnAprv.set_enable(true);
        			}
        			//결재문서조회
        			if( btnDocQ == "Y" ){
        				this.divData.form.btnAprvConnect.set_enable(true);
        			}
        			//결재초기화
        			if( btnInit == "Y" ){
        				this.divData.form.btnInit.set_enable(true);
        			}
        			//저장버튼 -> 21.10.21 -> 저장중이 아니면 저장버튼 비활성화
        			 // --사직서 상태(0:저장중,1:신청,2:완료)
        			if( this.dsList.getColumn(0, "CD_STATE") != 0 ){
        				this.divData.form.btnSave.set_enable(false);
        			}
        			/**************************************  버튼 컨트롤 (END)  **********************************/

        			/**************************************  그리드 컨트롤 (START)  **********************************/
        			//cdButtonStats  "1" 이상이거나 9보다 작으면 그리드를 보여주고 아니면 숨긴다 (0 , 9)
        			if( cdButtonStats > 0 &&  cdButtonStats < 9 ){
        				//this.divData.form.staTile02_00_00_00_00_00.set_visible(true);
        				//this.divData.form.objGrid.set_visible(true);
        				//담당자 결재 조회
        				//this.fnApprovalSelect();
        			}else if( cdButtonStats == 0 || cdButtonStats == 9 ){
        				this.divData.form.staTile02_00_00_00_00_00.set_visible(false);
        				this.divData.form.objGrid.set_visible(false);
        			}
        			//문서 작성자와 조회자가 같을때만 퇴직담당자 지정 팝업 호출
        			if( this.dsSearch.getColumn(0, "DS_HNAME") == this.AuthClient.DS_HNAME ){
        				this.fnPopup();
        			}
        		}
        		/**************************************  그리드 컨트롤 (END)  **********************************/
        		//텍스트 동적으로 사이즈 조절
        		if( !this.gfnIsNull(this.dsList.getColumn(0, "DS_HABIE"))){
        			this.divData.form.staDS_HABIE.set_text(this.dsList.getColumn(0, "DS_HABIE"));
        			//자동 폭 조절
        			this.divData.form.staDS_HABIE.set_fittocontents("width");
        			this.divData.form.divData2.set_left("staDS_HABIE:0");
        		}

        		if( !this.gfnIsNull(this.dsList.getColumn(0, "DS_RETIRE_DEPT"))){
        			this.divData.form.staDS_RETIRE_DEPT.set_fittocontents("width");
        			this.divData.form.staDesc24.set_left("staDS_RETIRE_DEPT:0");
        		}

        		if( !this.gfnIsNull(this.dsList.getColumn(0, "DS_RETIRE_ADMIN"))){
        			this.divData.form.staDS_RETIRE_ADMIN.set_fittocontents("width");
        		}

        		if (!this.gfnIsNull(this.dsList.getColumn(0, "DS_BANKS"))) {
        			this.divData.form.staDS_BANKS.set_fittocontents("width");
        		}
        		this.divData.form.resetScroll();

        		//스크롤 설정 초기화
        		this.resetScroll();
        	}
        	//저장완료 후
        	else if( svcID == "save" ){

        		//처음 저장 시는 정상 처리 후 팝업창 close 아닌건 재조회
        		if( this.dsSearch.getColumn(0, "NO_SEQ" ) == 0 ){
        			var param = {};
        			param.gubun = "Y";
        			this.getParentContext().close(JSON.stringify(param));
        		}else{
        			//신청 시
        			if( this.apply ){
        				//신청여부 false 처리
        				this.apply = false;
        				//저장 후 신청
        				this.fnApply();
        			}
        			//임금 및 퇴직금 지급 동의서 동의 후 결재상신 바로 눌렀을때
        			else if(this.approve){
        				this.dsSelect.clearData();
        				this.dsSelect.addRow();

        				this.dsSelect.setColumn(0, "CD_CORP", this.dsSearch.getColumn(0, "CD_CORP"));
        				this.dsSelect.setColumn(0, "ID_SABUN", this.dsSearch.getColumn(0, "ID_SABUN"));
        				this.dsSelect.setColumn(0, "NO_SEQ", this.dsSearch.getColumn(0, "NO_SEQ"));

        				var strSvcId    = "approvalDoc";
        				var strSvcType  = "approval/templete/DAW01";
        				var inProc      = "_dsProc";
        				var inData      = "payUpdate=dsSelect";
        				var outData     = "dsAppType=DAW01";
        				var strArg      = "";
        				var callBackFnc = "fnCallback";

        				this.gfnTransaction(strSvcId, strSvcType, inProc, inData, outData, strArg, callBackFnc);
        			}
        			else{
        				this.gfnAlert('정상 처리 되었습니다.');
        				this.fnSelect();
        			}
        		}
        	}
        	//삭제 완료 후
        	else if( svcID == "delete" ){
        		this.gfnAlert('정상 처리 되었습니다.');
        		var param = {};
        		param.gubun = "Y";
        		this.getParentContext().close(JSON.stringify(param));
        	}
        	//저장완료 후
        	else if( svcID == "apply" ){
        		this.gfnAlert('정상 처리 되었습니다.');
        		this.fnSelect();
        	}
        	//담당자 결재 조회완료 후
        	else if( svcID == "approvalSelect" ){
        		//결재 seq
        		this.approvalSeq = 0;								//팝업에 던져질 key 값
        		//var approvalUser = this.dsSearch.getColumn(0, "ID_SABUN");		//팝업에서 받아온 유저
        		var approvalUser = this.dsSearch.getColumn(0, "SIGN_ID_SABUN");		//팝업에서 조회 조건의 유저
        		var approvalCdStatus = "";							//결재 상태 코드(0:결재전, 1:결재완료, 2:결재보류, 9:기각)

        		//로그인 유저와 결재 인원이 일치하며 결재완료가 아닐때 셋팅
        		for( i = 0; i < this.dsApprovalList.rowcount; i ++ ){
        			var user = this.dsApprovalList.getColumn(i, "ID_SABUN");
        			var status = this.dsApprovalList.getColumn(i, "CD_STATS");

        			if( user == approvalUser && status !=1 ){
        				this.approvalSeq = this.dsApprovalList.getColumn(i, "NO_SEQ");
        			}
        		}
        		//this.approvalSeq 가 초기값 0이 아니면 결재버튼 활성화
        		if( this.approvalSeq != 0 ){
        			this.divData.form.btnApproval.set_visible(true);
