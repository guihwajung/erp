    <Script type="xscript5.1"><![CDATA[this.objApp = this.gfnGetApplication();
this._BEFORE_CD_SITE = "";

this.form_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	// -- 필수 -------------------//
	this.gfnFormOnLoad(this);	
	this.gfnFormInfo(this);		
	// ---------------------------//
	
	this.fnSetButton();
	this.fnSetExtendButton();
	this.fnSetVariable();
	this.fnSetEvent();
	this.fnSetParameter();
	
	//this.fnSetCombo();
	
	// 신규 작성화면일때
	if(this.getOwnerFrame().SCREEN_GB == "I"){
		// 타화면에서 오픈시 넘어오는 파라미터 처리
		if(!this.gfnIsNull(this.getOwnerFrame().DS_CONTMETHOD) /*&& !this.gfnIsNull(this.getOwnerFrame().NO_FORM)*/) {		
			this.dsList.set_enableevent(false);
			this.dsList.setColumn(0, "DS_CONTMETHOD", this.getOwnerFrame().DS_CONTMETHOD);
 			this.dsList.setColumn(0, "DS_CODE", this.getOwnerFrame().DS_CODE);
			
			this.dsList.setColumn(0, "CD_SITE", this.AuthClient.CD_SITE);
			this.dsList.setColumn(0, "DS_SITE", this.AuthClient.DS_SITE);
			
			// 현장 코드파인드 변경시 변경되었는지 체크하기 위한 전역변수.
			this._BEFORE_CD_SITE = this.AuthClient.CD_DEPT;
			
			this.dsList.set_enableevent(true);
			
			if(this.getOwnerFrame().DS_CONTMETHOD == "40")
			{
				this.divData.form.divData01.form.divTaDa02_1.form.sta30.set_text("품목");
				this.divData.form.divData01.form.divTaDa02_1.form.cfDS_COST.CodeFindName = "DMX_CFLICCOST";
				this.divData.form.divData01.form.divTaDa02_1.form.cfDS_COST.BeforeUserDataSetParam = "fnBeforeUserDataSetParam";
				this.divData.form.divData01.form.divTaDa04.form.cfDS_VENDOR.CodeFindName = "DCX_CFVENDOR_DM";
			}
			
			this.divData.form.divData01.form.divTaDa00.form.sta28.set_text("");
			this.divData.form.divData01.form.divTaDa00.form.cfTY_GUBUN_S.set_visible(false);
			
			this.fnSetYN_E_CONT_Changed("Y", this.getOwnerFrame().DS_CONTMETHOD, true);
			
			this.fnSelectSite();
		}
	}else{	// 조회 및 수정화면일때
		if(!this.gfnIsNull(this.getOwnerFrame().NO_CONT) && !this.gfnIsNull(this.getOwnerFrame().SN_SEQ)) {
			this.dsSearch.setColumn(0, "NO_CONT", this.getOwnerFrame().NO_CONT);
			this.dsSearch.setColumn(0, "SN_SEQ", this.getOwnerFrame().SN_SEQ);
			this.dsList.setColumn(0, "CD_SITE", this.getOwnerFrame().CD_SITE);
			
			this.ccfNO_CONT.form.fnCodeFindLoad();
			this.ccfSN_SEQ.form.fnCodeFindLoad();

			this.FormBtns.Select.click();
		}	
	}	
	

};

