}


this.fnSetYN_E_CONT_Changed = function(yn_e_cont, ds_contmethod, checkSelect)
{
	var height = 0;
	var height_dscontmethod = 0;
	var height_15 = 0;
	var height_16 = 0;
	var height_19 = 0;
	var height_20 = 0;

	if(yn_e_cont == "Y")
	{
		//this.divData.form.divData01.form.divTaDa02_1.form.sta28.set_textDecoration("underline");
		//this.divData.form.divData01.form.divTaDa02.form.sta28.set_textDecoration("underline");	
		if(ds_contmethod == "20" || ds_contmethod == "30")
		{
			height = 30;
			height_15 = 0;
			height_16 = 0;
			height_19 = 130;
			height_20 = 120;
		}
		else
		{
			height = 30;
			height_dscontmethod = 30;
			height_15 = 80;
			height_16 = 50;
			height_19 = 130;
			height_20 = 120;
		}
	}
	else
	{
		// 조회인 경우에는 패스.
		if(checkSelect != true)
		{
			this.dsList.setColumn(0, "NO_FORM", "");
			this.dsList.setColumn(0, "DS_FORM", "");
			
			this.dsList.setColumn(0, "CD_LICCOST", "");
			this.dsList.setColumn(0, "DS_COST", "");
			
			this.dsList.setColumn(0, "CD_PARTNERSIGN", "");
			this.dsList.setColumn(0, "DS_PARTNERSIGN", "");
		}
		this.divData.form.divData01.form.divTaDa02_1.form.sta28.set_textDecoration("none");
		this.divData.form.divData01.form.divTaDa02.form.sta28.set_textDecoration("none");
	}
	
	this.divData.form.divData01.form.divTaDa07.set_height(height);
	this.divData.form.divData01.form.divTaDa08.set_height(height);
	this.divData.form.divData01.form.divTaDa09.set_height(height);
	this.divData.form.divData01.form.divTaDa10.set_height(height_dscontmethod);
	this.divData.form.divData01.form.divTaDa11.set_height(height_dscontmethod);
	this.divData.form.divData01.form.divTaDa12.set_height(height_dscontmethod);
	this.divData.form.divData01.form.divTaDa13.set_height(height);
	this.divData.form.divData01.form.divTaDa15.set_height(height_15);
	this.divData.form.divData01.form.divTaDa16.set_height(height_16);
	this.divData.form.divData01.form.divTaDa17.set_height(height_dscontmethod);
	this.divData.form.divData01.form.divTaDa18.set_height(height_dscontmethod);
	this.divData.form.divData01.form.divTaDa19.set_height(height_19);
	this.divData.form.divData01.form.divTaDa20.set_height(height_20);
	
	this.divData.form.divData01.form.resetScroll();
}
