//코드파인드의 파라미터 값을 넘겨줄때 사용하는 함수
this.fnBeforeUserDataSetParam = function(id, dsUserParam, nrow) {
	
	if (id == "ccfNO_CONT") {
		if (this.gfnIsNull(this.dsList.getColumn(0, "CD_SITE"))) {
 			this.gfnAlert("현장코드를 먼저 입력하세요.");
 			return false;
 		}
		
		dsUserParam.setColumn(nrow, "CD_SITE", this.dsList.getColumn(0, "CD_SITE"));
	}
	else if (id == "ccfSN_SEQ"){
 		if (this.gfnIsNull(this.dsSearch.getColumn(0, "NO_CONT"))) {
 			this.gfnAlert("계약번호를 먼저 입력하세요.");
 			return false;
 		}
  
		dsUserParam.setColumn(nrow, "VALUE1", this.dsSearch.getColumn(0, "NO_CONT"));
	}
	else if (id == "cfNO_FORM"){
 		if (this.gfnIsNull(this.dsList.getColumn(0, "DS_CONTMETHOD"))) {
 			this.gfnAlert("계약종류를 먼저 입력하세요.");
 			return false;
 		}

 		if (this.gfnIsNull(this.dsList.getColumn(0, "CD_LICCOST"))) {
 			this.gfnAlert("품목코드를 먼저 입력하세요.");
 			return false;
 		}

		var sn_seq = this.dsSearch.getColumn(0, "SN_SEQ");
		
		if(this.gfnIsNull(this.dsSearch.getColumn(0, "SN_SEQ")))
		{
			sn_seq = "001";
		}
  
		dsUserParam.setColumn(nrow, "CD_LICCOST", this.dsList.getColumn(0, "CD_LICCOST"));
		dsUserParam.setColumn(nrow, "TY_CONT", this.dsList.getColumn(0, "DS_CONTMETHOD"));
		dsUserParam.setColumn(nrow, "SN_SEQ", sn_seq);
	}
	else if (id == "cfDS_COST"){
		dsUserParam.setColumn(nrow, "COSTCLASS", "");
	}
	else if(id == "cfCD_SITE"){
		this._BEFORE_CD_SITE = this.dsList.getColumn(0, "CD_SITE");
	}
	/*
	else if (id == "cfCD_ACCOUNT"){
 		if (this.gfnIsNull(this.dsSearch.getColumn(0, "CD_SITE"))) {
 			this.gfnAlert("현장코드를 먼저 입력하세요.");
 			return false;
 		}
  
		dsUserParam.setColumn(nrow, "TY_CONTH", this.dsList.getColumn(0, "DS_CONTMETHOD"));
		dsUserParam.setColumn(nrow, "CD_SITE", this.dsSearch.getColumn(0, "CD_SITE"));
	}
	*/
	return true;
}

this.fnAfterCDTextChanged = function(id, codeFindData){
	var arr = codeFindData;
	
	if(id == "cfYN_PATNERSIGN") {
		//this.ccfNO_BID.form.fnCodeFindClear();
		if(arr.length > 0)
		{
			var yn_partnersign = arr[0]["DS_ETC1"];
		
			this.dsList.setColumn(0, "YN_PARTNERSIGN", yn_partnersign);
			
			if(yn_partnersign == "Y")
			{
				this.divData.form.divData01.form.divTaDa02.form.cboYN_PARTNERSIGN.set_visible(true);
			}
			else
			{
				this.divData.form.divData01.form.divTaDa02.form.cboYN_PARTNERSIGN.set_visible(false);
			}
		}
	}
	else if(id == "cfCD_SITE")
	{
		if(arr[0]["CD_SITE"] != this._BEFORE_CD_SITE)
		{
			this._BEFORE_CD_SITE = arr[0]["CD_SITE"];
			this.fnSelectSite();
		}
	}
	else if(id == "cfDS_VENDOR")
	{
		if(arr.length > 0)
		{
			this.dsList.setColumn(0, "DS_DAMDANG", arr[0]["DS_NM_BID"]);
			this.dsList.setColumn(0, "DS_DAM_HANDPHONE", arr[0]["NO_HP_BID"]);
			this.dsList.setColumn(0, "DS_EMAIL_DAMDANG", arr[0]["DS_EM_BID"]);
		}
		else
		{
			this.dsList.setColumn(0, "DS_DAMDANG", "");
			this.dsList.setColumn(0, "DS_DAM_HANDPHONE","");
			this.dsList.setColumn(0, "DS_EMAIL_DAMDANG","");
		}
	}
}

