this.fnSelectAfter = function()
{
	// YN_EDITABLE 값에 따라 수정 가능 or 불가능
	if(this.dsList.getColumn(0, "YN_EDITABLE") == "Y"){			
		this.fnSetDivData01Enable(true);
	}else{
		this.fnSetDivData01Enable(false);
	}
	
	// 자재인 경우 공종을 품목으로 변경 및 codefind 변경. (외주인 경우 기본값으로)
	if(this.dsList.getColumn(0, "DS_CONTMETHOD") == "40")
	{
		this.divData.form.divData01.form.divTaDa02_1.form.sta30.set_text("품목");
		this.divData.form.divData01.form.divTaDa02_1.form.cfDS_COST.CodeFindName = "DMX_CFLICCOST";
		this.divData.form.divData01.form.divTaDa02_1.form.cfDS_COST.BeforeUserDataSetParam = "fnBeforeUserDataSetParam";
	}
	
	// 공동서명 관련.
	if(this.dsList.getColumn(0, "CD_PARTNERSIGN") == "D")
	{
		this.divData.form.divData01.form.divTaDa02.form.cboYN_PARTNERSIGN.set_visible(true);
	}
	else
	{
		this.divData.form.divData01.form.divTaDa02.form.cboYN_PARTNERSIGN.set_visible(false);
	}
	
	// 변경구분 관련.
	if(nexacro.toNumber(this.dsSearch.getColumn(0, "SN_SEQ"),0) > 1)
	{
		this.divData.form.divData01.form.divTaDa00.form.sta28.set_text("변경구분");
		this.divData.form.divData01.form.divTaDa00.form.cfTY_GUBUN_S.set_visible(true);
		
		// 자재일때와 외주일때의 코드파인드가 다름.
		if(this.dsList.getColumn(0, "DS_CONTMETHOD") == 40)
		{
			this.divData.form.divData01.form.divTaDa00.form.cfTY_GUBUN_S.CodeFindName = "CF_CODE_DM_CH10";
		}
		else
		{
			this.divData.form.divData01.form.divTaDa00.form.cfTY_GUBUN_S.CodeFindName = "CF_CODE_DC_CH10";			
		}
		
		// null이 아니라면 변경구분의 명칭을 가져오기 위해 codefindLoad.
		if(!this.gfnIsNull(this.dsList.getColumn(0, "TY_GUBUN_S")))
		{
			this.divData.form.divData01.form.divTaDa00.form.cfTY_GUBUN_S.form.fnCodeFindLoad();
		}
	}
	else
	{
		this.divData.form.divData01.form.divTaDa00.form.sta28.set_text("");
		this.divData.form.divData01.form.divTaDa00.form.cfTY_GUBUN_S.set_visible(false);
	}
	
	if(this.dsList.getColumn(0, "YN_ID_VENDOR") == "N")
	{
		this.divData.form.divData01.form.divTaDa04.form.sta05.set_color("red");
	}
	else
	{
		this.divData.form.divData01.form.divTaDa04.form.sta05.set_color("black");	
	}
	
	//this.fnSetDS_CONTMETHOD_Changed(this.dsList.getColumn(0, "DS_CONTMETHOD"));
	// 계약방법 관련.
	this.fnSetYN_E_CONT_Changed(this.dsList.getColumn(0, "YN_E_CONT"), this.dsList.getColumn(0, "DS_CONTMETHOD"), true);
	
	// 인지세 관련.
	if(this.dsList.getColumn(0, "YN_INJISE") == "N")
	{
		this.divData.form.divData01.form.divTaDa07.form.ccboCD_INJI.set_width(151);
		if(this.dsList.getColumn(0, "CD_INJI") == "C")
		{
			this.divData.form.divData01.form.divTaDa07.form.ctxtDS_INJI.set_width(100);
		}
		else
		{
			this.divData.form.divData01.form.divTaDa07.form.ctxtDS_INJI.set_width(0);
		}

		this.divData.form.divData01.form.divTaDa07.form.resetScroll();
	}
	else
	{
		this.divData.form.divData01.form.divTaDa07.form.ccboCD_INJI.set_width(0);			
		this.divData.form.divData01.form.divTaDa07.form.ctxtDS_INJI.set_width(0);
		this.divData.form.divData01.form.divTaDa07.form.resetScroll();
	}
	
	if(this.dsList.getColumn(0, "YN_CANCEL") == "Y")
