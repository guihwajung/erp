        		} else if(e.columnid == "DT_JOIN") {
        			var DT_JOIN_GROUP = this.dsData.getColumn(0, "DT_JOIN_GROUP");

        			this.dsData.clearData();
        			this.dsData.addRow();
        			this.dsData.setColumn(0, "DT_JOIN_GROUP", DT_JOIN_GROUP);
        		}
        	}
        }

