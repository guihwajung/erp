this.fnSetVariable = function() {
	this.ccfCD_SITE = this.divSearch.form.ccfCD_SITE;
	this.ccfID_DAM = this.divSearch.form.ccfID_DAM;

	this.dxGrid = this.divData.form.divDataTop.form.objGrid;
