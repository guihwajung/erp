this.fnsetGridCssCellProperty = function() {

	var nCellIdxAM_CURRENT1_C = this.dxGrid.getBindCellIndex("body", "AM_CURRENT1_C");
	var nCellIdxAM_CURRENT2_C = this.dxGrid.getBindCellIndex("body", "AM_CURRENT2_C");
