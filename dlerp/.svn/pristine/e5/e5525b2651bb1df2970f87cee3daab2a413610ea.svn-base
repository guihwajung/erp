		case "objGrid19":	// 동호회
			if (colnm == "DT_END") {
				var DT_START = objDs.getColumn(row, "DT_START");
				var DT_END = objDs.getColumn(row, "DT_END");
				if (DT_START > DT_END) {
					this.gfnAlert('연수종료일은 연수시작일과 같거나\n연수시작일보다 늦어야 합니다.');
					objDs.setColumn(row, "DT_END", "");
				}
			}
			break;
		case "objGrid20":	// 동호회
			if (colnm == "DT_CLUB_TO") {
				var DT_START = objDs.getColumn(row, "DT_CLUB_FROM");
				var DT_END = objDs.getColumn(row, "DT_CLUB_TO");
				if (DT_START > DT_END) {
					this.gfnAlert('종료일자는 가입일자와 같거나\n가입일자보다 늦어야 합니다.');
					objDs.setColumn(row, "DT_CLUB_TO", "");
				}
			}
			break;
	}	
}

