this.fileup_onitemchanged = function(obj:nexacro.FileUpload,e:nexacro.FileUploadItemChangeEventInfo) {

	if(obj.filelist.length > 0) {
		var filename = obj.filelist[0].filename;
		var fileext = filename.toLocaleLowerCase().substring(filename.lastIndexOf('.')+1);
		if(!this.gfnIsExistInArray(this.fileConfig.allowTypes, fileext)) {
			this.gfnAlert("이미지 파일(" + this.fileConfig.allowTypes.join(',') + ")을 선택하세요.");
			return;
		}
		
		if(!this.chkTxtPattern(filename, 0)) {
			this.gfnAlert("사진 파일명은 사번으로만 가능 합니다.");
			return false;
		}
		if(obj.filelist[0]._handle.size > 1048576) {
			this.gfnAlert("올리는 사진 파일 용량은 1MB를 초과할 수 없습니다.");
			return false;
		}
		if(fileext != "jpg") {
			this.gfnAlert("사진파일은 JPG 파일만 가능합니다.");
			return false;
		}
		
		var sFilePath = this.fileup.value;
		var dirExpt = sFilePath.lastIndexOf("\\")+1;
		var sFileName = sFilePath.substr(dirExpt);

		var key = this.dsList.getColumn(0, "ID_PERSON");
		var sUploadUrl = encodeURI(this.fileConfig.host + this.fileConfig.uploadUrl + "|" + key);	//id_person 값을 path에 포함시켜보냄.
		//var sUploadUrl = this.fileConfig.host.replace(/\\\\/g, "\\") + this.fileConfig.uploadUrl.replace(/\\\\/g, "\\") + "|" + key;
		this.fileup.upload(sUploadUrl);

		this.dsList.setColumn(0, "PIC", key+"."+fileext);

		//this.fnImageView();
		this.txtStatus = "U";
		this.gfnSetFormStatus(this, "U"); 
		
		this.fnSaveTab1();
	}
};

