this.chkTxtPattern = function(text, pattern){
    var chk = false;
    var pattern1 = /[^가-힣]/;             //한글 허용 할때
    var pattern2 = /[^a-zA-Z]/;            //영문 허용
    var pattern3 = /[^가-힣a-zA-Z0-9]/;   //한글,숫자,영문 허용
	var pattern4 = /[^a-zA-Z0-9](.?)/;    //(.)점, 숫자,영문 허용.
	
	if(pattern == 1){
	   var expression = RegExp(pattern1);
	}
	else if(pattern == 2){
	   var expression = RegExp(pattern2);
	}
	else if(pattern == 3){
	   var expression = RegExp(pattern3);
	}
	else{
	   var expression = RegExp(pattern4);
	}
	
    if (expression.test(text)){
        chk = true;
    }else{
        chk = false;
    }
	
	return chk;
	
};


