	else if(id == "ccfCD_CORP_ORD"){ //발령법인
		this.ccfCD_DEPT.form.fnCodeFindClear(); //발령부서 클리어
		this.ccfCD_DISPATCHORG.form.fnCodeFindClear(); //파견부서 클리어
	}
	
