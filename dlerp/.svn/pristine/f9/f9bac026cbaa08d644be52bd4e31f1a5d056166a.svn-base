        	var today = this.gfnGetDate();
        	var dt_from = this.gfnAddDate(today, -7);
        	this.divSearch.form.ctclDT_FROM.set_value(dt_from);

        	this.FormBtns.Select.click();
        };

        /************************************************************************
         * 버튼 설정
