         */
        this.fnCallback = function(svcID, errorCode, errorMsg) {
        	// 업무목표 중간점검 대상자 조회
        	if (svcID == "select_goal_bgvdprsn") {
        		this.gfnGridAfterSelect(this.dxGrid);
        		this.fnClearSummary();
        		this.gfnGridClear(this.dxGridSub);
        	}
        	// 업무목표 중간점검 조회
        	else if (svcID == "select_goal_check") {
        		var items = ['DS_GOAL_WRK', 'DS_GOAL_LVL_MIN', 'DS_GOAL_LVL_MAX', 'DS_HGHYR_ACPT', 'DS_LOWHYR_PLN'];
        		var list = [];
        		for (var i = 0; i < this.dsListGoalCheck.rowcount; i++) {
        			var max = 0;
        			for (var j = 0; j < items.length; j++) {
        				var item = this.dsListGoalCheck.getColumn(i, items[j]) || "";
        				var line = item.split("\n").length;
        				if (line > max) {
        					max = line;
        				}
        			}
        			list.push(max);
        		}

        		for (var i = 0; i < list.length; i++) {
        			if (list[i] == 2) {
        				this.dxGridSub.setRealRowSize(i, 40);
        			} else if (list[i] > 2) {
        				this.dxGridSub.setRealRowSize(i, 55);
        			}
        		}

        		// 대상자
        		this.staBGVDPRSN_ID_SABUN_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "ID_SABUN_BGVD"));		// 사번
        		this.staBGVDPRSN_DS_HNAME_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_HNAME_BGVD"));		// 성명
        		this.staBGVDPRSN_DS_POSITION_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_POSITION_BGVD"));	// 직급
        		this.staBGVDPRSN_DS_EMPTYPE_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_EMPTYPE_BGVD"));	// 직종
        		this.staBGVDPRSN_DS_DEPT_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_DEPT_BGVD"));			// 평가조직
        		this.staBGVDPRSN_DS_DUTY_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_DUTY_BGVD"));			// 직책

        		// 면담자
        		this.staITVWPRSN_ID_SABUN_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "ID_SABUN_ITVW"));		// 사번
        		this.staITVWPRSN_DS_HNAME_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_HNAME_ITVW"));		// 성명
        		this.staITVWPRSN_DS_POSITION_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_POSITION_ITVW"));	// 직급
        		this.staITVWPRSN_DS_EMPTYPE_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_EMPTYPE_ITVW")); 	// 직종
        		this.staITVWPRSN_DS_DEPT_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_DEPT_ITVW"));			// 평가조직
        		this.staITVWPRSN_DS_DUTY_bg.set_text(this.dsListGoalCheckSummary.getColumn(0, "DS_DUTY_ITVW"));			// 직책

        		// 직원 요청사항/면담자 피드백
        		this.txtDS_STAF_OPNN.set_value(this.dsListGoalCheckOpinion.getColumn(0, "DS_STAF_OPNN"));
        		this.txtDS_FDBK.set_value(this.dsListGoalCheckSummary.getColumn(this.dsListGoalCheckSummary.rowposition, "DS_FDBK"));

        		// 버튼 상태 변경
        		this.fnCheckExtensionButtonStatus();

        		// 버튼 활성화
        		this.fnEnableButton();
        	}
        	// 업무목표 중간점검 저장
        	else if (svcID == "save_goal_check") {
        		this.fnSelectGoalCheck();
        	}
        	// 업무목표 중간점검 제출
        	else if (svcID == "submit_goal_check") {
        		this.fnSelectGoalCheck();
        	}
        	// 업무목표수립 수정 요청
        	else if (svcID == "request_goal_check_modify") {
        		this.fnSelectGoalCheck();
        	}
        	// 업무목표수립 면담 완료
        	else if (svcID == "complete_goal_check_interview") {
        		this.fnSelectGoalCheck();
        	}
        }

