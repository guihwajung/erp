			//this.ccfID_RESIDENT.form.CDTextBox.set_value(arr[0]["ID_RESIDENT"]);
			//this.ccfID_RESIDENT.form.DSTextBox.set_value(arr[0]["DS_HNAME"]);		
			
			if(arr.length > 0) {
				this.edtID_SABUN.set_value(arr[0]["ID_SABUN"]);
			}else{
