		case 0:
		case 1:
		case 2:
			this.gfnGridBeforeSelect(this.dxGrid12);
			strSvcId    = "select1";
			inData      = "select1=dsSelect select12=dsSelect12 select_chrpsn=dsSelectChrpsn";
