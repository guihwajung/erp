this.fnGetTapIndex = function()
{
	var index = -1;

	var text = this.divData.form.tabData.tabpages[this.divData.form.tabData.tabindex].text;
	
	switch(text)
	{
		case "공사개요" : index = 0;
			break;
		case "계약정보" : index = 1;
			break;
		case "공사규모" :	index = 2;
			break;
		case "발주처" :	index = 3;
			break;
		case "공동도급" :	index = 4;
			break;
		case "현장인원" :	index = 5;
			break;
		case "하도대금지급보증서" :	index = 6;
			break;
		case "도급/실행" :	index = 7;
			break;
		case "청구/수금" :	index = 8;
			break;
		case "계획/공정" :	index = 9;
			break;
	}
	
	return index;
}



