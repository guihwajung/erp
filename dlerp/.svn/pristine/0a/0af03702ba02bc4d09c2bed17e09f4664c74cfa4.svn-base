        	this.gfnFormOpen("DCA", "DCA_REGISTDETAIL_R", "", param);
        }

        this.fnPreviewContFile = function() {
        	var NO_CONT = this.dsList.getColumn(this.dsList.rowposition, "NO_CONT");
        	var SN_SEQ = this.dsList.getColumn(this.dsList.rowposition, "SN_SEQ");

        	//https://erp.dsmec.com/report/previewContfile.do
        	window.open("https://erp.dsmec.com/report/previewContfile.do?NO_CONT=" + NO_CONT + "&SN_SEQ=" + SN_SEQ);
        }
        });
        
        // Regist UI Components Event
