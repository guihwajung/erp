              <Div id="ccfCD_SITE" CodeFindName="DZX_CFSITE" CDTextWidth="80" taborder="0" left="staCD_SITE:0.0" top="10.0" width="250" height="24.0" url="cmm::cmmCodeFind.xfdl"/>
              <Static id="sta00" taborder="2" text="기성년도" left="ccfCD_SITE:0.0" top="10.0" width="92.0" height="24.0" cssclass="sta_WF_SchLabelE"/>
              <Div id="ctcYR_WORK" taborder="3" left="sta00:0.0" top="10.0" width="100" height="24.0" url="cmm::cmmCalYY.xfdl" enable="true"/>
            </Layout>
