/************************************************************************************************
 * FORM 변수 선언 영역
 ************************************************************************************************/
this.fvFirstGap = 47;
this.fvBtnGap = 0;
this.fvTabHeight = 41;
this.fvTabExtraRightGap = 0;
this.fvExtraWidth = 32;
this.fvExtraTop = 0;
this.fvExtraHeight = 41;
this.fvExtraBtnPrefix = "EXTRA_";
this.fvTabBtnPrefix = "TAB_";
