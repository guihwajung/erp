this.fnSubmit_callback = function() {
	var cnt = 0;
	for (var i = 0; i < this.dsListGoalCheck.rowcount; i++) {
		var flag = this.gfnGetFlag(this.dsListGoalCheck, i);
		switch (flag) {
		case "I":
		case "U":
		case "D":
			cnt++;
			break;
		}
	}
	
	if (cnt > 0) {
		this.gfnAlert("저장되지 않은 업무목표가 존재합니다");
