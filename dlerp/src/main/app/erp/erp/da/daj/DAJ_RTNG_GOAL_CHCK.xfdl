				this.dsSaveGoalCheck.setColumn(nrow, "DS_EXEC_PLN_LOWHYR"  , this.gfnNvl(this.dsListGoalCheck.getColumn(i, "DS_EXEC_PLN_LOWHYR"), "").replace(/\v/g,"\n"));
