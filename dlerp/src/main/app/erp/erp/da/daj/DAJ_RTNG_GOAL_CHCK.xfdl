this.fnGridAfterEdit = function(obj:nexacro.NormalDataset, e:nexacro.DSColChangeEventInfo) {
console.log('fnGridAfterEdit', e);
	if (e.columnid == "NO_COL") {	// 순번
		if (e.oldvalue != e.newvalue) {
			var oldRow = e.row;
			var oldNO_COL = e.oldvalue;
			var newNO_COL = e.newvalue;
			var newRow = oldRow;
			for (var i = 0; i < obj.rowcount; i++) {
				if (oldRow == i) continue;
				var no_col = obj.getColumn(i, "NO_COL");
				if (no_col == newNO_COL && newNO_COL == 1) {
					newRow = 0;
					break;
				} else if (no_col < newNO_COL) {
					newRow = i + (oldNO_COL > newNO_COL ? 1 : 0);
				}
			}
			if (newRow >= obj.rowcount) newRow -= 1;
			
			obj.moveRow(oldRow, newRow);
			this.dxGridSub.setCellPos(0, newRow);
			
			var no_col = newNO_COL;
			this.dsListGoalCheck.set_enableevent(false);
			for (var i = newRow; i < this.dsListGoalCheck.rowcount; i++) {
				var flag = this.gfnGetFlag(this.dsListGoalCheck, i);
				var oldNO_COL = this.dsListGoalCheck.getColumn(i, "NO_COL");
				if (no_col != oldNO_COL) {
					if (flag != "I" && flag != "D") {
						this.dsListGoalCheck.setColumn(i, this.ucFlag, "U");
					}
					this.dsListGoalCheck.setColumn(i, "NO_COL", no_col);
				}
				no_col++;
			}
			this.dsListGoalCheck.set_enableevent(true);
		}
	}
	//this.btnSave.set_enable(true);
	this.gfnSetFormStatus(this, "U");
}

this.getNextNoCol = function() {
	var MAX = 0;
	for (var i = 0; i < this.dsListGoalCheck.rowcount; i++) {
		if (this.dsListGoalCheck.getColumn(i, "NO_COL") > MAX) {
			MAX = this.dsListGoalCheck.getColumn(i, "NO_COL");
		}
	}
	return MAX + 1;
}

