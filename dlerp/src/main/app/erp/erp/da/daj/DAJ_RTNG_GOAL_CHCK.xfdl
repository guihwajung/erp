		this.dsSaveGoalCheckOpnion.setColumn(0, "DS_STAF_OPNN", this.txtDS_STAF_OPNN.value);
		this.dsSaveGoalCheckOpnion.setColumn(0, "ID_USER"     , this.AuthClient.ID_USER);
	}
	else	// 평가자
	{
		this.dsSaveGoalInterviewer.clearData();
