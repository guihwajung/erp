				this.dsSaveGoalCheck.setColumn(nrow, "SEQ_BGVDPRSN"        , this.dsListGoalBgvdprsn.getColumn(this.dsListGoalBgvdprsn.rowposition, "SEQ_BGVDPRSN"));
				this.dsSaveGoalCheck.setColumn(nrow, "SEQ_GOAL"            , this.dsListGoalCheck.getColumn(i, "SEQ_GOAL"));
				this.dsSaveGoalCheck.setColumn(nrow, "NO_COL"              , this.dsListGoalCheck.getColumn(i, "NO_COL"));
				this.dsSaveGoalCheck.setColumn(nrow, "YN_SBMS"             , this.dsListGoalCheck.getColumn(i, "YN_SBMS"));
