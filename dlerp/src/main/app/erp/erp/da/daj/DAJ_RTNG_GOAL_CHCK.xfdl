				this.dsSaveGoalCheck.setColumn(nrow, "DS_RMK"              , this.dsListGoalCheck.getColumn(i, "DS_RMK"));
				this.dsSaveGoalCheck.setColumn(nrow, "DS_HGHYR_ACPT"       , this.gfnNvl(this.dsListGoalCheck.getColumn(i, "DS_HGHYR_ACPT"), "").replace(/\v/g,"\n"));
				this.dsSaveGoalCheck.setColumn(nrow, "TYPE_HGHYR_ACPT_RTNG", this.dsListGoalCheck.getColumn(i, "TYPE_HGHYR_ACPT_RTNG"));
				this.dsSaveGoalCheck.setColumn(nrow, "DS_LOWHYR_PLN"       , this.gfnNvl(this.dsListGoalCheck.getColumn(i, "DS_LOWHYR_PLN"), "").replace(/\v/g,"\n"));
				this.dsSaveGoalCheck.setColumn(nrow, "ID_USER"             , this.AuthClient.ID_USER);
				this.dsSaveGoalCheck.setColumn(nrow, "TY_PTCP"			   , this.dsSearch.getColumn(0, "TY_PTCP"));
				break;
			}
		}
		
		this.dsSaveGoalCheckOpnion.clearData();
		this.dsSaveGoalCheckOpnion.addRow();
		this.dsSaveGoalCheckOpnion.setColumn(0, "CD_CORP"     , this.dsSearch.getColumn(0, "CD_CORP"));
