// 추가 버튼 클릭 이벤트
this.btnAdd_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo) {
	
	var nrow = this.dsListGoalCheck.rowcount;
		
	if(this.dsListGoalCheck.rowcount == 0) {
		var nrow = this.gfnGridAdd(this.dxGridSub, "bottom");
		this.dsListGoalCheck.setColumn(nrow, "NO_COL", 1);
	} 
	else if(this.dsListGoalCheck.rowposition >= 0 && this.dsListGoalCheck.rowposition == nrow-1) {
		var NextCol = this.getNextNoCol();
		var nrow = this.gfnGridAdd(this.dxGridSub, "bottom");
		this.dsListGoalCheck.set_enableevent(false);
		this.dsListGoalCheck.setColumn(nrow, "NO_COL", NextCol);
		this.dsListGoalCheck.set_enableevent(true);
	}
	else {
		var curNO_COL = this.dsListGoalCheck.getColumn(this.dsListGoalCheck.rowposition, "NO_COL");
		var nrow = this.gfnGridAdd(this.dxGridSub, "current");
		
		this.dsListGoalCheck.set_enableevent(false);
		this.dsListGoalCheck.setColumn(nrow, "NO_COL", curNO_COL);
		
		for(var i = nrow+1; i < this.dsListGoalCheck.rowcount; i++) {
			var flag = this.gfnGetFlag(this.dsListGoalCheck, i);
			if(flag != "I" && flag != "D") {
				this.dsListGoalCheck.setColumn(i, this.ucFlag, "U");
			}
			this.dsListGoalCheck.setColumn(i, "NO_COL", ++curNO_COL);
		}
		this.dsListGoalCheck.set_enableevent(true);
	}
	this.gfnSetFormStatus(this, "I");
	this.btnDetail.set_enable(false);
	this.btnSave.set_enable(true);
}

