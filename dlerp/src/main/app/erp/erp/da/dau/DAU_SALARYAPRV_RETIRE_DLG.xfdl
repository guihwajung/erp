	var AprvCnt    = this.dsDetail.getColumn(0, "BTN_APRV");
	
	this.divTop.form.btnAprv.set_enable(AprvCnt == "Y");	// 결재상신
	this.divBtns.form.btnSave.set_enable(AprvCnt == "Y");	// 저장 버튼
	
