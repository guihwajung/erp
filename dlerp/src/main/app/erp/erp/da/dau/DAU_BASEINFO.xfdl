	this.gfnGridBeforeSelect(this.dxGrid);
	//this.gfnSetFormStatus(this);
	
	var id_sabun = this.dsSearch.getColumn(0, "ID_SABUN"); //this.divSearch.form.ccfID_SABUN.value;
