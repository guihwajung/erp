				this.dsDelete.setColumn(nrow, "ID_SABUN",  this.dsList.getColumn(i, "ID_SABUN"));
				break; 
		}
	}

	if (this.dsInsert.rowcount == 0 && this.dsUpdate.rowcount == 0 && this.dsDelete.rowcount == 0) return;

	var strSvcId    = "save";
	var strSvcType  = "save";
	var inProc		= "_dsProc";
