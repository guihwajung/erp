	this.dsSelect.setColumn(0, "CD_CORP"  , this.dsSearch.getColumn(0, "CD_CORP"));
				
	var strSvcId    = "select";
	var strSvcType  = "grid";
	var inProc		= "_dsProc";
	var inData      = "selectBase=dsSelect select=dsSelect selectSub=dsSelect";
	var outData     = "dsBase=selectBase0 dsList=select0 dsListSub=selectSub0";
