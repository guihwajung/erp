this.fnSetVariable = function() {	
	this.divSplitter = this.divData.form.divSplitter;
	this.divDataRight = this.divData.form.divDataRight;
	
	this.dxGrid = this.divData.form.divDataLeft.form.objGrid;
};

/************************************************************************
