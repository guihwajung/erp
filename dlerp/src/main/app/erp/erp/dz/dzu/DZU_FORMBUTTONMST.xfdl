this.fnSetVariable = function() {
	this.ccfMODULE = this.divSearch.form.ccfMODULE;
	this.cboNM_DB = this.divSearch.form.cboNM_DB;
	this.dxGrid = this.divData.form.objGrid;
}

/************************************************************************
