this.fnSetExtendButton = function() {
	//this.btn1 = this.gfnFormButtonAdd("Detail", "fnDetail");
	this.btnDeploy = this.gfnFormButtonAdd("Deploy", "fnDeploy");
	this.btnFormButton = this.gfnFormButtonAdd("FormButton", "fnFormButton"); 
};

/************************************************************************
