				this.divData.form.tabData.tab1.form.ctxtRT_ARCH.set_readonly(true);
				this.divData.form.tabData.tab1.form.ctxtAM_LAND00.set_readonly(true);
				this.divData.form.tabData.tab1.form.ctxtAM_LAND05.set_readonly(true);
			} else {
