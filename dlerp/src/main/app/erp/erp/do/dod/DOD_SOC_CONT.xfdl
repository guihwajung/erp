			this.FormBtns.Select.click();
		} else if(svcID == "delete"){
			this.fnDeleteAfter = function() {
				this.FormBtns.Select.click();
			}
			this.gfnAlert("삭제되었습니다.", "fnDeleteAfter");
		}
	} else {
		this.gfnAlert(errorMsg);
	}
};

