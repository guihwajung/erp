				this.dsDelete.setColumn(nrow, "CD_JOB", this.dsList.getColumn(i, "CD_PRIMARY"));
				break;
		}
	}
	
	if (this.dsInsert.rowcount == 0 && this.dsUpdate.rowcount == 0 && this.dsDelete.rowcount == 0) return;
		
	var strSvcId    = "save";
	var strSvcType  = "save";
	var inProc		= "_dsProc";
