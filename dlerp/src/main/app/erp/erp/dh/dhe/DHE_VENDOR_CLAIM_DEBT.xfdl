              <Calendar id="ctclDT_FROM" taborder="3" left="staDT_FROM:0.0" top="staCD_CORP:10.0" height="24.0" dateformat="yyyy-MM-dd" editformat="yyyyMMdd" autoselect="true" width="105" onchanged="divSearch_ctclDT_FROM_onchanged"/>
