              <Static id="staDT_GIJUN" taborder="0" text="기준일자" left="ccfCD_SITE:0.0" top="10.0" width="92.0" height="24.0" cssclass="sta_WF_SchLabelE"/>
              <Calendar id="ctclDT_GIJUN" taborder="1" left="staDT_GIJUN:0.0" top="10.0" width="110" height="24.0" dateformat="yyyy-MM-dd" editformat="yyyyMMdd" autoselect="true"/>
            </Layout>
