	this.ccfCD_CORP = this.divSearch.form.ccfCD_CORP;
	this.ccfCD_SITE = this.divSearch.form.ccfCD_SITE;
	this.chkKOOKMIN_YESU_CHECK = this.divData.form.divData02.form.chkKOOKMIN_YESU_CHECK;
