			switch(chkKookminStatus){
				case 0:
					this.chkKOOKMIN_YESU_CHECK.set_value(false);
				break;
				case 1:
					this.chkKOOKMIN_YESU_CHECK.set_value(true);
				break;
			}
			
			switch(chkHealthStatus){
