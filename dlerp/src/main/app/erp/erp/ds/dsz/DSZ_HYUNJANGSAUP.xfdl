	this.Apply_KOOKMIN = this.gfnFormButtonAdd("Apply_KOOKMIN", "fnApply_KOOKMIN"); //국민연금공단 계좌정보 등록
	this.Apply_HEALTH = this.gfnFormButtonAdd("Apply_HEALTH", "fnApply_HEALTH"); //건강보험공단 계좌정보 등록
};

/************************************************************************
 * 변수 선언
 ************************************************************************/
