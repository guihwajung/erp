    <Layouts>
      <Layout>
        <Div id="divData" taborder="0" left="0" top="0" bottom="0" right="0" cssclass="div_DATA_Bg">
          <Layouts>
            <Layout>
              <Grid id="objGrid" taborder="0" left="0" top="55" bottom="0" right="0"/>
              <Static id="staDS_NAME" taborder="1" text="성명 : " left="5" top="32" height="22" textAlign="left" width="35"/>
              <Static id="staDS_NAME_INFO" taborder="2" left="42" top="32" height="22" textAlign="left" width="238" text="(~)"/>
              <Static id="staTopTitle" taborder="3" text="월별 출역일수" left="0" top="0" height="22" cssclass="sta_TITLE_Bg" width="186"/>
            </Layout>
          </Layouts>
        </Div>
