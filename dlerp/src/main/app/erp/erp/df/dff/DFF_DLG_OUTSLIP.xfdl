this.fnSetParameter = function() {
	this.mskAM_EZA = this.divData.form.mskAM_EZA;			// 이자금액
	this.mskAM_CORP_JUMIN = this.divData.form.mskAM_CORP_JUMIN;		// 법인세/주민세
