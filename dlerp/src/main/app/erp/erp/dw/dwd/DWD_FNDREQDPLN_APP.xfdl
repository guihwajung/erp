	this.dxGrid.EnterCell = "fnGrid_EnterCell";
	this.dxGridSub.EnterCell = "fnGrid_EnterCell";
	this.dxGridSub.AfterEdit = "fnSubGrid_AfterEdit";		
	
	// 결재관련
	this.gfnGridInit(this.dxGridAppr, this.dsListAplines, "DZ", "DZR_APLINES_01");
