	this.ccfCD_SITE = this.divSearch.form.ccfCD_SITE;
	this.tclYR_WORK = this.divSearch.form.tclYR_WORK;
	this.rdoTY_QUERY = this.divSearch.form.rdoTY_QUERY;
	this.dxGrid = this.divData.form.objGrid;
};

/************************************************************************
