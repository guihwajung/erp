		} else {
			this.gfnAlert(errorMsg);
		}
	}
	else if(svcID == "update_attend")
	{
		if (errorCode == 0) {
			this.fnExecute_callback = function()
			{
				this.FormBtns.Select.click();
			}
			
			this.gfnAlert("현설참석여부확인이 정상처리되었습니다.", "fnExecute_callback");
		}
		else
		{
			this.gfnAlert(errorMsg);
		}
	}
	else if(svcID == "insert_po")
