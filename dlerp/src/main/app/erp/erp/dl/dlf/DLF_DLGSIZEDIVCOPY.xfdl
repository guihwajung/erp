	this.dsSave.addColumn("NO_SIZE_ORIG", "bigdecimal");
	this.dsSave.addColumn("TY_TYPE_ORIG", "string");
	this.dsSave.addColumn("CD_ITEM_ORIG", "string");
	this.dsSave.addColumn("TY_COPY", "string");
