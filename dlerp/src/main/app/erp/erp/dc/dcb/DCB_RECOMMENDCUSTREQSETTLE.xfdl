/************************************************************************
 * Validate
 ************************************************************************/
this.fnAppValidate = function() {
	var validate = true;
	
	if (this.dsList.rowcount = 0) return false;
	else if(this.dsList2.rowcount == 0)
	{
		this.gfnAlert("지명업체를 등록해주십시오.");
		return false;
	}
	
	if(this.gfnGetFormStatus(this) == "U")
	{
		this.gfnAlert("저장 후 지명품의를 진행해주십시오.");
		return false;
	}
	
	for (var i = 0; i < this.dsList2.rowcount; i++) {
		var flag = this.gfnGetFlag(this.dsList2, i);
		if (flag == "I" || flag == "U" || flag == "D") {
			this.gfnAlert("저장 후 지명품의를 진행해주십시오.");
			return false;
		}
	}
	
// 	// 면허 필수체크.
// 	for(var i = 0; i < this.dsList2.rowcount; i++)
// 	{
// 		if(this.gfnIsNull(this.dsList2.getColumn(i, "CD_LICENSE")))
// 		{
// 			this.gfnAlert("입찰참여업체의 면허를 선택해주세요.");
// 			return false;
// 		}
// 	}
	
	return validate;
};

