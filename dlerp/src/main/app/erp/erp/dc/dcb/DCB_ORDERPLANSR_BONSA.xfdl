				this.dsList.setColumn(0, "DT_EXPLAIN", "");
				this.dsList.setColumn(0, "AT_SITEEXPLAIN", "");
				this.dsTempTime.setColumn(0, "EXPLAIN_HOUR", "");
				this.dsTempTime.setColumn(0, "EXPLAIN_MINUTE", "");
			} else {
				this.divData.form.tabData.tab1.form.div00.form.tclDT_EXPLAIN.set_enable(true);
				this.divData.form.tabData.tab1.form.div00.form.ccboDT_EXPLAIN_HOUR.set_enable(true);
				this.divData.form.tabData.tab1.form.div00.form.ccboDT_EXPLAIN_MINUTE.set_enable(true);
				this.divData.form.tabData.tab1.form.div00.form.txtAT_SITEEXPLAIN.set_enable(true);
			}
		}
		else if(e.columnid == "YN_PREPAY1")
		{
			if(e.newvalue == "Y") {
				this.divData.form.divData01.form.txtDS_PREPAY1.set_enable(true);
			} else {
				this.dsList.setColumn(0, "DS_PREPAY1", "");
				this.divData.form.divData01.form.txtDS_PREPAY1.set_enable(false);
			}
		}
		else if(e.columnid == "ONOFF")
		{
			if(e.newvalue == "ON") {
				obj.setColumn(e.row, "AT_BIDMAGAM", "온라인 입찰");
			} else {
				obj.setColumn(e.row, "AT_BIDMAGAM", "본사 입찰");
			}
		}
		else if(e.columnid == "ONOFF_EXPLAIN")
		{
			if(e.newvalue == 9) {
				this.dsList.setColumn(0, "AT_SITEEXPLAIN", "");
				this.dsList.setColumn(0, "DT_EXPLAIN", "");
				this.dsTempTime.setColumn(0, "EXPLAIN_HOUR", "");
				this.dsTempTime.setColumn(0, "EXPLAIN_MINUTE", "");
