this.fnSelectValidate = function() {
	var validate = true;
	if (this.gfnIsNull(this.dsSearch.getColumn(0,"CD_SITE"))) {
		validate = false;
		// Alert후 실행할 처리
		this.fnVaidateCallback = function() {
			this.divSearch.form.ccfCD_SITE.form.CDTextBox.setFocus();
		}
		this.gfnAlert("현장코드를 입력하세요.", "fnVaidateCallback");
	}
	else if (this.gfnIsNull(this.dsSearch.getColumn(0,"NO_BID"))) {
		validate = false;
		// Alert후 실행할 처리
		this.fnVaidateCallback = function() {
			this.divSearch.form.ccfNO_BID.form.CDTextBox.setFocus();
		}
		this.gfnAlert("입찰번호를 입력하세요.", "fnVaidateCallback");
	}
	return validate;
};

this.fnSaveValidate = function()
{
	var validate = true;
	var msg = "";
	
	var err = this.gfnDataValidate(this.dsList, this.dsRequired, false);
	if (!this.gfnIsNull(err))
	{
		validate = false;
		msg += err;
	}
	
	var d = new Date();
	var s = d.getFullYear()
	  + ((d.getMonth() + 1) + "").padLeft(2, '0')
	  + (d.getDate() + "").padLeft(2, '0')
	  + (d.getHours() + "").padLeft(2, '0')
	  + (d.getMinutes() + "").padLeft(2, '0');
	  
	if(this.dsList.getColumn(0, "ONOFF_EXPLAIN") != "9")
	{
		if(this.gfnIsNull(this.dsList.getColumn(0, "DT_EXPLAIN")))
		{
			validate = false;
			msg += "[현설일시] 필수입력값입니다.\n";
		} else {
			if(this.gfnIsNull(this.dsTempTime.getColumn(0, "EXPLAIN_HOUR")) || this.gfnIsNull(this.dsTempTime.getColumn(0, "EXPLAIN_MINUTE"))) {
				msg += "[현설일시] 시, 분을 입력해 주시기 바랍니다.";
				validate = false;
			} else {
				if(this._DT_EXPLAIN != this.dsList.getColumn(0, "DT_EXPLAIN") + this.dsTempTime.getColumn(0, "EXPLAIN_HOUR") + this.dsTempTime.getColumn(0, "EXPLAIN_MINUTE")) {
					if(this.dsList.getColumn(0, "DT_EXPLAIN") + this.dsTempTime.getColumn(0, "EXPLAIN_HOUR") + this.dsTempTime.getColumn(0, "EXPLAIN_MINUTE") < s) {
						msg += "[현설일시] 현재 일시 이후로 등록 하십시오.";
						validate = false;
					}
				}
			}		
		}
		
		if(this.gfnIsNull(this.dsList.getColumn(0, "ID_SITESABUN"))) {
			validate = false;
			msg += "[현장담당자] 필수입력값입니다.";			
		}
		
		if(this.gfnIsNull(this.dsList.getColumn(0, "NO_SITETEL"))) {
			validate = false;
			msg += "[현장담당자 전화번호] 필수입력값입니다.";			
		}		
	}
	
	if(!this.gfnIsNull(this.dsList.getColumn(0, "DT_BIDMAGAM"))) {
		if(this.gfnIsNull(this.dsTempTime.getColumn(0, "BIDMAGAM_HOUR")) || this.gfnIsNull(this.dsTempTime.getColumn(0, "BIDMAGAM_MINUTE"))) {
			msg += "[입찰마감일시] 시, 분을 입력해 주시기 바랍니다.";
			validate = false;
		} else {
			if(this._DT_BIDMAGAM != this.dsList.getColumn(0, "DT_BIDMAGAM") + this.dsTempTime.getColumn(0, "BIDMAGAM_HOUR") + this.dsTempTime.getColumn(0, "BIDMAGAM_MINUTE")) {
				if(this.dsList.getColumn(0, "DT_BIDMAGAM") + this.dsTempTime.getColumn(0, "BIDMAGAM_HOUR") + this.dsTempTime.getColumn(0, "BIDMAGAM_MINUTE") < s) {
					msg += "[입찰마감일시] 현재 일시 이후로 등록 하십시오.";
					validate = false;
				}
			}
		}
	}	
	
	switch(this.dsList.getColumn(0, "DS_CONTMETHOD"))
	{
		case "00" :
		case "10" :
		case "20" :
		case "30" : 
			var err = this.gfnDataValidate(this.dsList, this.dsRequired_DC, false);
			if (!this.gfnIsNull(err))
			{
				validate = false;
				msg += err;
			}
			
			/*
			if(this.fnZeroCheck(this.dsList.getColumn(0, "AREA_SITE1")) && this.fnZeroCheck(this.dsList.getColumn(0, "AREA_SITE2"))
			&& this.fnZeroCheck(this.dsList.getColumn(0, "AREA_SITE3")) && this.fnZeroCheck(this.dsList.getColumn(0, "AREA_SITE4")))
			{
				msg += "[1공구 ~ 4공구연면적] 중 하나는 필수입력값입니다.\n";
				validate = false;
			}
			*/
		break;
	}
	
	if(!this.gfnIsNull(msg)) {
		var arrError = [msg];
		this.gfnAlert("msg.err.validator", arrError);
	}
	
	return validate;
}


