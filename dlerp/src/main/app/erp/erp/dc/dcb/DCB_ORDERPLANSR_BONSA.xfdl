    <Script type="xscript5.1"><![CDATA[this.objApp = this.gfnGetApplication();
this.singFormStat = "Q";	// 싱글폼 데이터 수정시 변경됨(싱글폼데이터와 그리드데이터를 같이 저장할지 각각 따로 저장할지 구분하기 위한변수)
this._READONLY = "";
