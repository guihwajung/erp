	}
	
	var nRow = this.dsList3.rowposition;
	
	var param = {};
	
	param.CD_SITE = this.dsList3.getColumn(nRow, "CD_SITE");
	param.DS_SITE = this.dsList3.getColumn(nRow, "DS_SITE");
