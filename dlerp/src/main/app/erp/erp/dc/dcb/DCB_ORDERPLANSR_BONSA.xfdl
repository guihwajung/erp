this.fnPRAttach2 = function(obj:nexacro.Button,e:nexacro.ClickEventInfo) {
	if (!this.fnSelectValidate()) return false;
	
	if(this.dsList3.rowcount == 0)
	{
		this.gfnAlert("현설정보가 존재 하지 않습니다.");
